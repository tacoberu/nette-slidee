{
	"name": "tacoberu/nette-slidee",
	"type": "library",
	"description": "Simple pages for Nette.",
	"keywords": [
		"webpage",
		"nette"
	],
	"homepage": "https://github.com/tacoberu/nette-slidee",
	"license": "MIT",
	"authors": [
		{
			"name": "Martin Takáč",
			"homepage": "http://martin.takac.name",
			"email": "martin@takac.name"
		}
	],
	"support": {
		"issues": "https://github.com/tacoberu/nette-slidee/issues"
	},
	"require": {
		"nette/application": "^2.3",
		"nette/bootstrap": "^2.3",
		"nette/utils": ">=2.3 <2.5",
		"tracy/tracy": "^2.3",
		"latte/latte": "^2.3"
	},
	"require-dev": {
		"phpunit/phpunit": "^4",
		"mikey179/vfsStream": "^1.6",
		"tacoberu/phpcs-coding-standard": "@dev",
		"sstalle/php7cc": "^1.1",
		"phpmd/phpmd": "^2.5"
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "git://github.com/tacoberu/phpcs-coding-standard"
		}
	],
	"autoload": {
		"psr-4": {
			"Taco\\Nette\\Slidee\\": "libs/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Taco\\Nette\\Slidee\\": "tests"
		}
	}
}
